{
  "name": "vue-image-viewer-mz",
  "version": "1.0.12",
  "private": false,
  "files": [
    "dist/*"
  ],
  "maintainers": [
    "bryce"
  ],
  "author": "bryce",
  "keywords": [
    "image viewer",
    "medium zoom",
    "vue2",
    "vue3"
  ],
