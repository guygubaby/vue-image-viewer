    "esbuild-register": "^2.6.0",
    "eslint": "^7.30.0",
    "rollup": "2.75.7",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "rollup-plugin-vue": "6.0.0",
    "sass": "1.34.1",
    "sass-loader": "8.0.2",
    "typescript": "4.1.5",
    "vue": "^3.0.0"
